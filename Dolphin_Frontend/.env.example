# Frontend Environment Variables Template
# Copy this to .env and fill in your values

# API Base URL - Your Render backend URL
VUE_APP_API_BASE_URL=https://your-backend.onrender.com

# App Name
VUE_APP_NAME=Dolphin

# Enable/disable debug mode
VUE_APP_DEBUG=false

# Stripe Public Key (if using Stripe)
VUE_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
