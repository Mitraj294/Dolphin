# Frontend Environment Variables Template
# Copy this to .env and fill in your values

# API Base URL - Local backend only
VUE_APP_API_BASE_URL=http://127.0.0.1:8000

# App Name
VUE_APP_NAME=Dolphin

# Enable/disable debug mode
VUE_APP_DEBUG=false

# Stripe Public Key (if using Stripe)
VUE_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
